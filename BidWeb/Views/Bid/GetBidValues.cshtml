@model IEnumerable<BidWeb.Models.BidPrices>

@{
    ViewData["Title"] = "GetBidPrices";
}

<h4>Bidders and Bid Values</h4>

@if (Model.Count()>0)
{
<table class="table table-striped">
    <thead class="table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BidValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AliasId)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BidValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AliasId)
            </td>
           
        </tr>
}
    </tbody>
</table>
}

else
{
    <p>No bidders yet..</p>
}

<div class="text-center">
    <button type="button" class="btn btn-dark" onclick="history.back()">Back</button>
</div>
